
@{
    /**/
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>


    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="pragma" content="no-cache" />
    <meta HTTP-EQUIV="Cache-Control" CONTENT="no-cache, must-revalidate">
    <meta http-equiv="expires" content="0" />
    <title>菲仕兰登录页</title>
    <link rel="icon" href="~/Img/Login/title_logo.png" type="image/x-icon">
    <link href="~/lib/layui/css/layui.css" rel="stylesheet" />
    <link href="~/css/Login/default.css" rel="stylesheet" />
    <link href="~/css/Login/demo.css" rel="stylesheet" />
    <link href="~/css/Login/loaders.css" rel="stylesheet" />
    <link href="~/css/Login/styles.css" rel="stylesheet" />
</head>

<body style=" background-size: 100%;background-image:url(/Img/Login/zzpic.jpg);background-repeat:no-repeat;background-size:100% 100% ">
    <form id="form1" runat="server">
        <div class='login'>
            <div class='login_title'>
                <img src="~/Img/Login/Title_Left.png" style="position: absolute; top: 25px;  left: 18%;height:100px;width:290px" />
                @*<span>登录页 </span>*@
            </div>
            <div id="loginType" style="display:none">qwertyuiopasdfghjklzxcvbnm!!</div>
            <div class='login_fields'>
                @Html.AntiForgeryToken()
                <div class='login_fields__user'>
                    <div class='icon'>
                        <img alt="" src='~/Img/Login/user_icon_copy.png'>
                    </div>
                    <input id="loginName" name="loginName" placeholder='请输入账号' maxlength="30" type='text' autocomplete="off" value="" runat="server" />
                    <div class='validation'>
                        <img alt="" src="~/Img/Login/tick.png" />
                    </div>
                </div>
                <div class='login_fields__password'>
                    <div class='icon'>
                        <img alt="" src="~/Img/Login/lock_icon_copy.png" />
                    </div>
                    <input name="pwd" placeholder='请输入密码' maxlength="16" type='text' autocomplete="off" value="" />
                    <div class='validation'>
                        <img alt="" src="~/Img/Login/tick.png" />
                    </div>
                </div>
                <div class='login_fields__password' style=" display:none">
                    <div class='icon'>
                        <img alt="" src="~/Img/Login/key.png" />
                    </div>
                    <input name="code" placeholder='请输入验证码' style="" maxlength="4" type='text' autocomplete="off" />
                    <div class='validation' style=" opacity: 1; right: -5px; top: -3px;">
                        <canvas class="J_codeimg" id="myCanvas" onclick="Code();">对不起，您的浏览器不支持canvas，请下载最新版浏览器!</canvas>
                    </div>
                </div>
                <div class='login_fields__submit'>
                    <input type='button' value='登录' />
                </div>
            </div>
            <div class='success'></div>
            <div class='disclaimer'>
                <p>欢迎登录MDM主数据平台</p>
            </div>
        </div>
        <div class='authent'>
            <div class="loader" style="height: 44px;width: 44px;margin-left: 28px;">
                <div class="loader-inner ball-clip-rotate-multiple">
                    <div></div>
                    <div></div>
                    <div></div>
                </div>
            </div>
            <p>认证中...</p>
        </div>
        <div class="OverWindows"></div>

        <script src="~/lib/Login/jquery-3.3.1.min.js"></script>
        <script src="~/lib/Login/jquery-ui.min.js"></script>
        <script src="~/lib/layui/layui.js"></script>
        <script src="~/lib/Login/stopExecutionOnTimeout.js"></script>
        <script src="~/lib/Login/Particleground.js"></script>
        <script src="~/lib/Login/Treatment.js"></script>
        <environment include="Development">
            <script src="~/js/dev/Login_Bak.js?v=@DateTime.Now.Millisecond"></script>
        </environment>
        <environment exclude="Development">
            <script src="~/js/dist/Login_Bak.js?v=@DateTime.Now.Millisecond"></script>
        </environment>
    </form>


</body>
</html>

